<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<startup>
		<!-- Target .NET Framework version -->
		<supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8" />
	</startup>

	<!-- Cấu hình chuỗi kết nối (nếu có kết nối SQL sau này) -->
	<connectionStrings>
		<add name="QLBHConnection"
			 connectionString="Data Source=.;Initial Catalog=QLBH;Integrated Security=True"
			 providerName="System.Data.SqlClient" />
	</connectionStrings>

	<!-- Cấu hình ứng dụng (có thể thêm key riêng) -->
	<appSettings>
		<add key="AppName" value="Quản Lý Bán Hàng" />
		<add key="Version" value="1.0.0" />
	</appSettings>
</configuration>
